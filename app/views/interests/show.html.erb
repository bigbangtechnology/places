<div data-role="header">
  <h1><%= @interest.name %></h1>
</div>
<div data-role="content">
  <ul data-role="listview">
    <% @distances.each do |distance| %>
    <li data-role="list-divider"><%= "#{distance} kms"%></li>
    <% @interest.places.each do |place| %>
    <li><%= place.name %></li>
    <% end %>
    <% end %>
  </ul>
</div>